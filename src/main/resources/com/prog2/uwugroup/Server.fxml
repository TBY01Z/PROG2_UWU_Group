<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.prog2.uwugroup.ServerStageStart">
   <children>
      <MenuBar layoutY="1.0" prefHeight="26.0" prefWidth="800.0">
        <menus>
          <Menu mnemonicParsing="false" text="Quit">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#closeServer" text="Close Server" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Server Info's">
            <items>
              <MenuItem mnemonicParsing="false" text="Kick" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#aboutInfo" text="About" />
                  <MenuItem mnemonicParsing="false" onAction="#portSelect" text="Select Port" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Label layoutX="386.0" layoutY="427.0" text="Server IP:" />
      <CheckBox layoutX="427.0" layoutY="455.0" mnemonicParsing="false" onAction="#startServer" text="Server start?" />
      <Label fx:id="ipLabel" layoutX="457.0" layoutY="427.0" text="255.255.255.255" />
      <Label layoutX="544.0" layoutY="427.0" text=":" />
      <Label fx:id="portLabel" layoutX="547.0" layoutY="427.0" prefHeight="17.0" prefWidth="52.0" text="0" />
   </children>
</AnchorPane>
